{
    "openapi": "3.0.0",
    "info": {
      "title": "API AgendaAi",
      "description": "Descrição",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost:3000",
        "description": "Servidor local"
      }
    ],
    "paths": {
      "/auth/signup": {
        "post": {
          "summary": "Cadastrar um novo usuário",
          "description": "Cadastra um novo usuário com email, senha e nome de usuário.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    },
                    "displayName": {
                      "type": "string"
                    }
                  },
                  "required": ["email", "password", "displayName"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Usuário cadastrado com sucesso."
            },
            "400": {
              "description": "Parâmetros inválidos ou faltantes."
            },
            "500": {
              "description": "Erro interno do servidor."
            }
          }
        }
      },
      "/auth/signin": {
        "post": {
          "summary": "Login de usuário",
          "description": "Login de usuário com email e senha.",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string"
                    },
                    "password": {
                      "type": "string"
                    }
                  },
                  "required": ["email", "password"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Usuário logado com sucesso."
            },
            "401": {
              "description": "Autenticação falhou."
            },
            "500": {
              "description": "Erro interno do servidor."
            }
          }
        }
      },
      "/lanchonetes/criar": {
        "post": {
          "summary": "Cadastrar de cantina",
          "description": "Cadastrar uma nova cantina no sistema",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "nome": {
                      "type": "string"
                    },
                    "cnpj": {
                      "type": "string"
                    },
                    "cep": {
                      "type": "string"
                    },
                    "logradouro": {
                      "type": "string"
                    },
                    "numero": {
                      "type": "string"
                    },
                    "bairro": {
                      "type": "string"
                    },
                    "cidade": {
                      "type": "string"
                    },
                    "estado": {
                      "type": "string"
                    }
                  },
                  "required": ["nome", "cnpj", "cep", "logradouro", "numero", "bairro", "cidade", "estado"]
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Lanchonete criada com sucesso"
            },
            "500": {
              "description":  "Erro ao criar lanchonete"
            }
          }
        }
      },
      "/listar": {
        "get": {
          "summary": "Listar lanchonetes",
          "description": "Recupera uma lista de lanchonetes.",
          "responses": {
            "200": {
              "description": "Lista de lanchonetes recuperada com sucesso"
            },
            "500": {
              "description": "Erro ao buscar lanchonetes"
            }
          }
        }
      },
      "/buscar/{id}": {
        "get": {
          "summary": "Buscar uma lanchonete pelo ID",
          "description": "Recupera uma lanchonete com base em seu ID.",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Lanchonete encontrada com sucesso"
            },
            "500": {
              "description": "Erro ao buscar lanchonete"
            }
          }
        }
      },
      "/deletar/{id}": {
        "delete": {
          "summary": "Deletar uma lanchonete pelo ID",
          "description": "Exclui uma lanchonete com base em seu ID.",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "204": {
              "description": "Lanchonete excluída com sucesso"
            },
            "404": {
              "description": "Lanchonete não encontrada"
            },
            "500": {
              "description": "Erro ao excluir lanchonete"
            }
          }
        }
      }
    }
  }
  